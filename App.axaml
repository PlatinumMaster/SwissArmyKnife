<Application xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="using:SwissArmyKnife"
             xmlns:themes="clr-namespace:Material.Styles.Themes;assembly=Material.Styles"
             xmlns:avalonia="clr-namespace:SwissArmyKnife"
             x:Class="SwissArmyKnife.App">
    <Application.DataTemplates>
    </Application.DataTemplates>

    <Application.Styles>
        <themes:MaterialTheme BaseTheme="Dark" PrimaryColor="LightBlue" SecondaryColor="Cyan" />
        <StyleInclude Source="avares://DialogHost.Avalonia/Styles.xaml"/>
        <StyleInclude Source="Styles/Navbar.axaml"></StyleInclude>
        <StyleInclude Source="Styles/Spinner.axaml"></StyleInclude>
        <StyleInclude Source="avares://AvaloniaEdit/Themes/Fluent/AvaloniaEdit.xaml" />
    </Application.Styles>
</Application>
